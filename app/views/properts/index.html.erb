<p id="notice"><%= notice %></p>

<h1>Listing Properts</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Name</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @properts.each do |propert| %>
      <tr>
        <td><%= propert.code %></td>
        <td><%= propert.name %></td>
        <td><%= propert.value %></td>
        <td><%= link_to 'Show', propert %></td>
        <td><%= link_to 'Edit', edit_propert_path(propert) %></td>
        <td><%= link_to 'Destroy', propert, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Propert', new_propert_path %>
